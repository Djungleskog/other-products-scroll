{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "better-styled-components"
  ],
  "rules": {
    "no-await-in-loop": "off",
    "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": [0, {}]
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
